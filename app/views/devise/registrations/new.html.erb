

<!--<%= render "devise/shared/links" %>-->
<div class="contact-bg overview-bgi">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <!-- Form content box start -->
                <div class="form-content-box">
                    <!-- logo -->
                    <a href="index.html" class="clearfix alpha-logo">
                        <img src="img/logos/white-logo.png" alt="white-logo">
                    </a>
                    <!-- details -->
                    <div class="details">
                        <h3>Create an account</h3>
                        <!-- Form start-->
                        <%= form_for(resource, as: resource_name, url: user_registration_path) do |f| %>
                        <%= devise_error_messages! %>
                            <div class="form-group">
                                <%= f.text_field :name, class: "input-text", placeholder:"user Name" %>
                            </div>
                            <div class="form-group">
                                <%= f.email_field :email, class: "input-text", placeholder: "Email Address" %>
                            </div>
                            <div class="form-group">
                                
                                <%= f.password_field :password, class: "input-text", placeholder: "Password" %>
                            </div>
                            <div class="form-group">
                                <%= f.password_field :password_confirmation, class: "input-text", placeholder: "Confirm Password" %>
                                <% if @minimum_password_length %>
                                <em>(<%= @minimum_password_length %> characters minimum)</em>
                                <% end %><br />
                            </div>
                            <div class="mb-0">
                                <%= f.submit "Sign up",class: "btn-md btn-theme btn-block" %>
                            </div>
                        <% end %>
                        <!-- Form end-->
                    </div>
                    <!-- Footer -->
                    <div class="footer">
                        <span>
                            Already a member? <a href="login.html">Login here</a>
                        </span>
                    </div>
                </div>
                <!-- Form content box end -->
            </div>
        </div>
    </div>
</div>